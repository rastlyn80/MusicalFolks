<%= form_tag search_path, class: "navbar-form navbar-left", method: :get do %>
    <div class="form-group">
        <%= text_field_tag :search, params[:search], placeholder: "Anywhere", class: "form-control", id: "autocompletion" %>
    </div>
    <div class="form-group">
        <%= submit_tag "Search", class: "btn btn-form" %>
    </div>
<% end %>

<script>
    $(function() {
        $("#autocompletion").geocomplete();
    });
</script>