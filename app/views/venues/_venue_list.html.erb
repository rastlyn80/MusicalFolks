<% venues.each do |venue| %>
    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading preview">
                <%= image_tag venue.cover_photo(:medium) %>
            </div>
            <div class="panel-body">
                <%= link_to venue.listing_name %>
            </div>
        </div>
    </div>
<% end %>